<% include ../partials/header.ejs %>

<link rel="stylesheet" href="../../css/list-of-dieticians.css">

        <div>
            <h3>List of Dieticians</h3>
        </div>
        <div class='row'>

              <% for(var i = 0; i < listOfDieticians.length; i++){ %>
                <div class='column'>
                  <div class="card">
                    <img src="./../../assets/avatar2.png" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title"><%= listOfDieticians[i].Fname%></h5>
                      <p class="card-text"><%= listOfDieticians[i].Specialization %></p>
                      <div class='button'>
                          <a href="/dietician-profile/<%= listOfDieticians[i].Did %>" class="btn btn-danger btn-lg" role="button" aria-pressed="true">More Info<img src="../../assets/forward-24px.svg"></a>
                      </div>
                      </div>
                  </div>
                </div>
              <% } %>
        </div>

